@page "/initialize"
@using Microsoft.AspNetCore.Identity
@using FimsCPK.Entities;
@using FimsCPK.Pages;

@inject NavigationManager NavManager
@inject ILogger<Initialize> Logger
@inject SignInManager<AspNetUser> SignInManager
@inject UserManager<AspNetUser> UserManager
@inject IHttpContextAccessor HttpContextAccessor

@code {

    protected override async Task OnInitializedAsync()
    //protected override void OnInitialized()
    {
        string strUser = UserManager.GetUserName(HttpContextAccessor.HttpContext.User);
        Logger.LogInformation("CPK관리 Loginned:user=" + strUser);

        NavManager.NavigateTo("/home");

    }

}
